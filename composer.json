{
    "name": "yanselmask/openable",
    "description": "This package is designed to simplify and automate the management of opening and closing hours for various establishments, such as stores, restaurants, gyms, or nightclubs.",
    "keywords": [
        "laravel",
        "framework",
        "Yanselmask",
        "Spatie",
        "Openable",
        "opening-hours",
        "restaurant",
        "hotel",
        "rentalcars",
        "rental",
        "shifts",
        "hours",
        "times",
        "dates",
        "opening-hours",
        "opening-days",
        "opening-months"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yanselmask",
            "email": "jansel48@hotmail.com"
        }
    ],
    "require": {
        "spatie/opening-hours": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Yanselmask\\Openable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yanselmask\\Openable\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
